<GridPane fx:id="mainPane" prefHeight="59.0" prefWidth="128.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <BorderPane prefHeight="60.0" prefWidth="104.0" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS">
         <left>
            <HBox alignment="CENTER_LEFT" prefHeight="60.0" prefWidth="148.0" spacing="4.0" BorderPane.alignment="CENTER">
               <children>
                  <StackPane prefHeight="60.0" prefWidth="33.0">
                     <children>
                        <ImageView fx:id="deleteRowButton" fitHeight="28.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/remove.png" />
                           </image>
                        </ImageView>
                     </children>
                  </StackPane>
                  <Label fx:id="ipText" prefHeight="36.0" prefWidth="110.0" text="255.255.255.255">
                     <font>
                        <Font size="14.0" />
                     </font></Label>
               </children>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </HBox>
         </left>
      </BorderPane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
