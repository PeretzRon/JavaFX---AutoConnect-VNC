<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<fx:root fx:id="main" prefHeight="400.0" prefWidth="650.0" styleClass="grid-pinger-popup" stylesheets="@../../../style/popupStyle.css" type="AnchorPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
         <StackPane fx:id="dusbinStackPane" layoutX="25.0" layoutY="20.0" prefHeight="30.0" prefWidth="30.0">
             <ImageView  fx:id="dusbinImageView" fitHeight="29.0" fitWidth="29.0">
                 <image>
                     <Image url="@../../../images/dustbin.png" />
                 </image>
             </ImageView>
         </StackPane>
        <GridPane>
            <HBox fx:id="buttonsHBox" alignment="CENTER" spacing="10" GridPane.halignment="CENTER" GridPane.rowIndex="0">
                <Button fx:id="saveChangesButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefWidth="70.0" styleClass="button-save-popup-grid" text="Save" />
                <Button fx:id="closeButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="20.0" prefWidth="70.0" styleClass="button-close-popup-grid" text="Close" />
                <Button fx:id="test" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="20.0" prefWidth="70.0" styleClass="button-close-popup-grid" text="Test" />
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </padding>
            </HBox>
            <FlowPane fx:id="mainPane" alignment="CENTER" prefHeight="381.0" prefWidth="760.0" GridPane.rowIndex="1">
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </padding>
            </FlowPane>
      <columnConstraints>
         <ColumnConstraints halignment="CENTER" />
      </columnConstraints>
      <rowConstraints>
         <RowConstraints />
         <RowConstraints />
      </rowConstraints>
        </GridPane>
</fx:root>
