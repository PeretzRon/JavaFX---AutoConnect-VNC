<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root fx:id="mainPane" alignment="CENTER" prefHeight="90.0" prefWidth="120.0" spacing="10.0" styleClass="pinger-item" stylesheets="@../style/itemStyle.css" type="HBox" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <VBox fx:id="mainVbox" alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="8.0">
      <children>
         <VBox id="label" alignment="CENTER" prefHeight="46.0" prefWidth="101.0">
            <children>
                <Label id="name" fx:id="name" styleClass="pinger-item" text="name" wrapText="true">
                  <font>
                     <Font size="13.0" />
                  </font>
               </Label>
            </children>
         </VBox>
         <HBox fx:id="LabelsHBox" alignment="CENTER" prefHeight="55.0" prefWidth="101.0" spacing="15.0">
            <children>
               <ImageView fx:id="editItem" fitHeight="20.0" fitWidth="20.0" onMouseClicked="#editPingGroupHandler" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="@../images/pencil.png" />
                  </image>
               </ImageView>
               <ImageView fx:id="deleteItem" fitHeight="20.0" fitWidth="20.0" onMouseClicked="#deletePingGroupHandler" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="@../images/dustbin.png" />
                  </image>
               </ImageView>
            </children>
         </HBox>
      </children>
   </VBox>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</fx:root>
